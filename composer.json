{
	"description": "The CodeIgniter framework",
	"name": "codeigniter/framework",
	"type": "project",
	"homepage": "https://codeigniter.com",
	"license": "MIT",
	"support": {
		"forum": "http://forum.codeigniter.com/",
		"wiki": "https://github.com/bcit-ci/CodeIgniter/wiki",
		"irc": "irc://irc.freenode.net/codeigniter",
		"source": "https://github.com/bcit-ci/CodeIgniter"
	},
	"require": {
		"php": ">=7.3",
		"predis/predis": "^1.1",
		"ext-json": "*",
		"phpunit/phpunit": "^9.5",
		"doctrine/orm": "^2.1"
	},
	"suggest": {
		"paragonie/random_compat": "Provides better randomness in PHP 5.x"
	},
	"require-dev": {
		"mikey179/vfsstream": "1.1.*"
	},
	"autoload": {
		"psr-4": {
			"App\\Config\\": "application/config",
			"App\\Controller\\": "application/controllers",
			"App\\Dto\\": "application/dtos",
			"App\\Library\\": "application/libraries",
			"App\\Service\\": "application/services",
			"App\\Tests\\": "tests",
			"App\\Helper\\": "application/helpers",
			"App\\Helper\\Traits\\": "application/helpers/traits"
		}
	}
}
